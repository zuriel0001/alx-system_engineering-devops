#!/usr/bin/env bash

#########################################
# Script: hours_and_minutes.sh
# Displays hours from 0 to 12 and minutes
# from 1 to 59 for each hour.
# Author: zuriel001
# Date: July 27, 2023
#########################################

i="0"
while [ $i -le 12 ]
do
echo "Hour: $i"
j="1"
while [ $j -le 59 ]
do
echo "$j"
j=$((j+1))
done
i=$((i+1))
done
